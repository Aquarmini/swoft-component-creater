{
    "name": "{{component}}",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "component"
    ],
    "description": "{{description}}",
    "authors": [
        {
            "name": "{{author}}",
            "email": "{{email}}"
        }
    ],
    "autoload": {
        "psr-4": {
            "{{namespace}}": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SwoftTest\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "swoft/framework": "^1.0.24"
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "dev-master",
        "phpunit/phpunit": "^5.7",
        "phpstan/phpstan": "^0.10.5"
    }
}
